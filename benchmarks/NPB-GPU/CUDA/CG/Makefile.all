llvm:
	$(MAKE) CLASS=S INJECT_CODE_LLVM=1
	$(MAKE) clean
	$(MAKE) CLASS=W INJECT_CODE_LLVM=1
	$(MAKE) clean
	$(MAKE) CLASS=A INJECT_CODE_LLVM=1
	$(MAKE) clean
	$(MAKE) CLASS=B INJECT_CODE_LLVM=1
	$(MAKE) clean 
