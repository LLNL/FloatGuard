SHELL=/bin/sh
BENCHMARK=cg
BENCHMARKU=CG

all:
	$(MAKE) CLASS=S
	$(MAKE) clean
	$(MAKE) CLASS=W
	$(MAKE) clean
	$(MAKE) CLASS=A
	$(MAKE) clean
	$(MAKE) CLASS=B
